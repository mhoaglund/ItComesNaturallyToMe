{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Electron App",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/app/main.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/.",
			"runtimeExecutable": "${workspaceRoot}/node_modules/electron-prebuilt/dist/electron.exe",
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": true,
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}